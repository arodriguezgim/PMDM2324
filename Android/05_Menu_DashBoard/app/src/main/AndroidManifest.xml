<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme._05_Menu_DashBoard"
        tools:targetApi="31">
        <activity
            android:name=".ui.SettingsActivity"
            android:theme="@style/SettingsTheme"
            android:exported="false" />
        <activity
            android:name=".ui.RestaurantesActivity"
            android:theme="@style/RestaurantesTheme"
            android:exported="false" />
        <activity
            android:name=".ui.MapasActivity"
            android:theme="@style/MapasTheme"
            android:exported="false" />
        <activity
            android:name=".ui.GaleriaActivity"
            android:theme="@style/GaleriaTheme"
            android:exported="false" />
        <activity
            android:name=".ui.QuizzesActivity"
            android:theme="@style/QuizzesTheme"
            android:exported="false" />
        <activity
            android:name=".ui.EdadCaninaActivity"
            android:theme="@style/PerreteTheme"
            android:exported="false" />
        <activity
            android:name=".ui.SuperHeroesActivity"
            android:theme="@style/SuperHeroesTheme"
            android:exported="false" />
        <activity
            android:name=".ui.CalculadoraActivity"
            android:theme="@style/CalculadoraTheme"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>